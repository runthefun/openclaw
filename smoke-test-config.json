{
  "agents": {
    "defaults": {
      "model": {
        "primary": "openrouter/anthropic/claude-sonnet-4.5"
      }
    }
  },
  "auth": {
    "profiles": {
      "openrouter:default": {
        "provider": "openrouter",
        "mode": "api_key"
      }
    },
    "order": {
      "openrouter": ["openrouter:default"]
    }
  },
  "plugins": {
    "enabled": false,
    "slots": {
      "memory": "none"
    }
  }
}
